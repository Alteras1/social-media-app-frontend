{
  "name": "social-media-app-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.0",
    "bootstrap": "^4.5.3",
    "@types/node-sass": "^4.11.1",
    "json-server": "^0.16.2",
    "node-sass": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router": "^5.2.0",
    "react-scripts": "4.0.0",
    "reactstrap": "^8.7.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.0.5",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "api": "json-server json-server/db.json --routes json-server/routes.json --no-cors=true --port 8000",
    "build:css": "node-sass src/ -o src/",
    "watch:css": "npm run build:css && node-sass src/ -o src/ -w -r",
    "start:ts": "react-scripts start",
    "start": "npm-run-all -p watch:css start:ts",
    "build:ts": "react-scripts build",
    "build": "npm-run-all build:*"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.19.3",
    "@types/react": "^16.9.55",
    "@types/react-dom": "^16.9.9",
    "@types/react-redux": "^7.1.9",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.6",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "react-router-dom": "^5.2.0"
  }
}
